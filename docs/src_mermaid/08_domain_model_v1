classDiagram
    User "*"-->"1" Contest
    Contest "1"-->"*" Question
    Contest "1"-->"*" Profile
    Contest "1"-->"*" Award
    Profile "1"-->"*" Award
    class Contest {
        - List~Question~ questions
        - List~Profile~ profiles
    }
    class Profile {
        - int points
        - List~Award~ awards
    }
    class Question {
        - String question
        - List~String~ answers
        - String correctAnswer
    }
    class Award {
        - int price
    }